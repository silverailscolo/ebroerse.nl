  "private": true,
  "scripts": {
    "clean": "bundle exec jekyll clean",
    "dev": "webpack --mode=development && bundle exec jekyll build && npm run delete-nondefault-img && npm run serve",
    "build": "webpack --mode=production && JEKYLL_ENV=production bundle exec jekyll build",
    "postbuild": "npm run delete-nondefault-img",
    "delete-nondefault-img": "rm -r _site/*/assets/img",
    "serve": "npx http-server -p 4001 _site/"
  },
