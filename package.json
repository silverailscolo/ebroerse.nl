  "devDependencies": {
    "@shopify/prettier-plugin-liquid": "^1.4.4",
    "prettier": "^3.2.5"
  },
  "dependencies": {
    "http-server": "^14.1.1",
    "isotope-layout": "^3.0.6",
    "jquery": "^3.7.1",
    "lightgallery": "^2.7.2"
  }
